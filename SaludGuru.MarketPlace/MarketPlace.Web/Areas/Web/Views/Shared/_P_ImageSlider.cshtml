@model MarketPlace.Models.Profile.ProfileViewModel

@if (!Model.RenderScripts)
{
    <div id="divProfileSlide">
        <!-- Slides Container -->
        <div u="slides" class="PrincipalSlide">
            @foreach (SaludGuruProfile.Manager.Models.Profile.ProfileInfoModel oImg in Model.CurrentProfile.ProfileInfo.Where(x => x.ProfileInfoType == SaludGuruProfile.Manager.Models.enumProfileInfoType.ImageGeneral).ToList())
            {
                <div>
                    <img u="image" src="@oImg.Value" style="width: 100%; height: auto;" />
                </div>
            }
        </div>

        <!-- ThumbnailNavigator Skin Begin -->
        <div u="thumbnavigator" class="jssort11" style="position: absolute;width: 105px;height: 300px;right: 0px;top: 0px;">
            <!-- Thumbnail Item Skin Begin -->
            <div u="slides" style="cursor: move;">
                <div u="prototype" class="p" style="position: absolute;width: 100%;height: 72px;top: 0;left: 0;">
                    <thumbnailtemplate style=" width: 100%; height: auto; border: none;position:absolute; top: 0; left: 0;"></thumbnailtemplate>
                </div>
            </div>
            <!-- Thumbnail Item Skin End -->
        </div>
    </div>
}
else
{
    <script type="text/javascript">
        $(document).ready(function () {
            ProfileSliderObject.Init({
                DivId: 'divProfileSlide',
            });
            ProfileSliderObject.RenderAsync();
        });
    </script>
}
