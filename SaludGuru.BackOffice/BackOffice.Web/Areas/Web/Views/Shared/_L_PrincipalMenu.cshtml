@model List<BackOffice.Models.General.MenuModel>

<!--principal menu-->
<ul>
    @foreach (BackOffice.Models.General.MenuModel PrincipalM in Model)
    {
        switch (PrincipalM.PrincipalMenu)
        {
            case BackOffice.Models.General.enumPrincipalMenu.Administrator:
                <li>
                    <a href="">Administrar</a>
                    <ul>
                        <li>
                            <a href="@Url.Action(MVC.Profile.ActionNames.Search,MVC.Profile.Name)">Perfil</a>
                        </li>
                        <li>
                            <a href="@Url.Action(MVC.Insurance.ActionNames.Search, MVC.Insurance.Name)">Seguros medicos</a>
                        </li>
                        <li>
                            <a href="@Url.Action(MVC.Specialty.ActionNames.Search, MVC.Specialty.Name)">Especialidades</a>
                        </li>
                        <li>
                            <a href="@Url.Action(MVC.Treatment.ActionNames.Search, MVC.Treatment.Name)">Tratamientos</a>
                        </li>
                    </ul>
                </li>
                break;
            case BackOffice.Models.General.enumPrincipalMenu.Appointment:
            <li>
                <a href="">Agenda</a>
                <ul>
                    <li>
                        <a href="">Día</a>
                    </li>
                    <li>
                        <a href="">Semana</a>
                    </li>
                    <li>
                        <a href="">Mes</a>
                    </li>
                    <li>
                        <a href="">Lista</a>
                    </li>
                    <li>
                        <a href="">Configuracion</a>
                    </li>
                </ul>
            </li>
                break;
            case BackOffice.Models.General.enumPrincipalMenu.Dashboard:
            <li>
                <a href="@Url.Action(MVC.Home.ActionNames.Dashboard,MVC.Home.Name)">Dashboard</a>
            </li>
                break;
            case BackOffice.Models.General.enumPrincipalMenu.Patient:
            <li>
                <a href="">Pacientes</a>
            </li>
                break;
            case BackOffice.Models.General.enumPrincipalMenu.Profile:
            <li>
                <a href="">Perfil</a>
            </li>
                break;
            default:
                break;
        }
    }
</ul>
